{
  "name": "knight-omega",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Modern React application with Skipper UI features, Vite, TypeScript, Tailwind CSS, and comprehensive UI components",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "antd",
    "radix-ui",
    "skipper-ui",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
